                      			// Find the correct password (Codestudio) August Code Rush Challenge Q.1 //
                      			// Given n strings containing passwords of odd size //
                      			// if there exist a reversed string in same then it is a correct password //
                      			// Print size and central character of correct password //
                      			// n=4, s={"las", "god", "psala", "sal"} => 3 a (las=sal) //
                      			// T.C = O(n), S.C = O(n) //

#include <bits/stdc++.h>
using namespace std;

int main() 
{
	// Write your code here
	int n;
	cin>>n;

	vector<string> passwords;

	for(int i=0;i<n;i++)
	{
		string p;
		cin>>p;
		
		passwords.push_back(p);
	}

	for(auto i: passwords)
	{
		string rev=i;
		reverse(rev.begin(), rev.end());

		if(find(passwords.begin(), passwords.end(), rev)!=passwords.end())
		{
			int size=i.size();
			char central=i[size/2];
			cout<<size<<" "<<central;
			break;
		}
	}
}
